{% block js %}
    <script src="/plugins/jquery.min.js?version=1.01"></script>
    <script src="/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="/plugins/slick/slick.js"></script>
    <script src="/js/custom.js"></script>
    <script src="/js/jquery.bxslider.js"></script>
    <script>
        $('.our-clients').find('.slider-container').slick({
            dots: true,
            arrows: false,
            infinite: true,
            centerMode: true,
            slidesToShow: 5,
            slidesToScroll: 3,
            responsive:[
                {
                    breakpoint: 991,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 2
                    }
                },
                {
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 1
                    }
                }
            ]
        });
        $('#jsSlickHome').slick({
            dots: false,
            arrows: true,
            infinite: true,
            centerMode: true,
            slidesToShow: 1,
            slidesToScroll: 1,
        });
    </script>
{% endblock %}

